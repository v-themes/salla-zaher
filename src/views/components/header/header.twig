{# Suggest mobile app #}
<div class="suggest-mobile-app flex justify-between items-center p-2.5 bg-stone-100 gap-3">
    <div class="flex-1 flex items-center gap-1">
        <div class="close text-gray-500 cursor-pointer text-sm">&#10006;</div>

        <img fetchpriority="high" loading="eager" class="w-14 h-16 object-contain shrink-0" src="{{theme.settings.get('app_image')|default(store.logo)}}" alt="{{ store.name }}">

        <div class="flex-1 flex flex-col my-1.5">
            <div class="font-semibold text-sm truncate w-[90%]">{{theme.settings.get('app_title')|default(store.name)}}</div>
            <p class="text-xs text-gray-500 w-[90%]">{{trans('blocks.header.promo')}}</p>
